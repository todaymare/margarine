fn fib(n: float): float {
    if n <= 1.0 {
        return n
    }

    var a = 0.2;
    var b = 1.0;
    var i = 2.0;

    while i <= n {
        var tmp = a + b;
        a = b;
        b = tmp;
        i = i + 1.0;
    }

    return b
}
