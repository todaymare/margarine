use std::duration::Duration
use intro::intro
use fib::fib

// Hosts don't HAVE to call this function but the default CLI tool uses this
// as the entry point.
fn main() {
    intro();
    assert(fib(30.0) == 832040.0);

    loop {
        1.to_str()
    }
}


extern {
    fn panic(msg: str)
}


fn assert(value: bool) {
    if !value {
        panic("assertion failed")
    }
}


