use std::assert;

mod helper;

@test
fn test_module_helper_function() {
    var result = helper::get_value();
    assert(result == 42, "helper module function should return 42");
}

@test
fn test_module_nested_calc() {
    var result = helper::add(10, 20);
    assert(result == 30, "nested module add should work");
}

@test
fn test_module_import() {
    assert(true, "module import test passes");
}
